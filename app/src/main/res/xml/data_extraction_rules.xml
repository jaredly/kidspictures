<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
	<cloud-backup>
		<!-- Exclude cache and temporary files -->
		<exclude domain="cache" />
		<exclude domain="file" path="temp/" />
	</cloud-backup>
	<device-transfer>
		<!-- Include user preferences -->
		<include domain="sharedpref" path="." />
	</device-transfer>
</data-extraction-rules>
